  ./etc/templates/MESH_parameters_CLASS.tpl      ; ./mesh/MESH_parameters_CLASS.ini
  ./etc/templates/MESH_parameters_hydrology.tpl  ; ./mesh/MESH_parameters_hydrology.ini
EndFilePairs

# extra directories
BeginExtraDirs
