# random seed is set to the Unix EPOCH time when this
# file is created

# essential variables
ProgramType       DDS
ModelExecutable   ./etc/scripts/model_evaluation.py
ModelSubdir       cpu_
ObjectiveFunction gcop

# useful optional variables
PreserveModelOutput  ./etc/scripts/archive.sh
OstrichWarmStart     yes
NumDigitsOfPrecision 3
TelescopingStrategy  none
RandomSeed           1730235264
OnObsError           1.00

# experimental or less common optional variables
CheckSensitivities    no
SuperMUSE             no
OstrichCaching        yes
BoxCoxTransformation  1.00
ModelOutputRedirectionFile  ostrich_output.txt

# DDS specs
BeginDDSAlg
  PerturbationValue 0.2
  MaxIterations      10000
  UseRandomParamValues
EndDDSAlg

# response variable
BeginResponseVars
  #name        filename                                  keyword      line   col   token
  KGE_2012     ./mesh/results/kge_2012.csv ;             OST_NULL     0      1     ','
EndResponseVars

# objective function specs
BeginGCOP
  CostFunction     KGE_2012
  PenaltyFunction  APM
EndGCOP

# file pairs
